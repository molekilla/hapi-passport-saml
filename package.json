{
  "name": "@aeroline_1025/hapi-corpsso",
  "version": "2.2.1",
  "description": "A Hapi plugin that wraps passport-saml for SAML SSO (as SP). This is a fork from hapi-passport-saml project (https://github.com/molekilla/hapi-passport-saml)",
  "main": "lib/index",
  "types": "lib/index",
  "scripts": {
    "start": "npm run build:live",
    "build:live": "nodemon --exec tsc -P tsconfig.json",
    "build": "tsc -P tsconfig.json",
    "pretest": "npm run build",
    "test": "lab"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "files": [
    "lib"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/gdelpu/hapi-corpsso.git"
  },
  "keywords": [
    "saml",
    "sso",
    "hapi"
  ],
  "author": "Gr√©gory DELPU <gregory.delpu@soprasteria.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gdelpu/hapi-corpsso/issues"
  },
  "homepage": "https://github.com/gdelpu/hapi-corpsso#readme",
  "dependencies": {
    "@hapi/boom": "^7.4.2",
    "debug": "^2.2.0",
    "passport-saml": "^1.1.0"
  },
  "devDependencies": {
    "@hapi/code": "^5.3.1",
    "@hapi/lab": "^19.1.0",
    "@hapi/hapi": "^18.3.1",
    "@types/hapi": "^15.0.2",
    "@types/node": "^10.14.10",
    "nodemon": "^1.19.1",
    "ts-node": "^7.0.1",
    "typescript": "^2.9.2"
  }
}
